{
  "name": "express-starter-new",
  "version": "1.0.0",
  "description": "Express modular framework",
  "main": "main.ts",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "dev": "nodemon --watch 'App/**/*.ts' --exec 'ts-node' App/main.ts",
    "start": "npm run build && NODE_ENV=production node ./dist/main.js",
    "test": "ENV=test jest --detectOpenHandles",
    "test:watch": "ENV=test yarn test --detectOpenHandles --watch",
    "rnc": "ts-node ./App/Utils/Cli/Cli.ts"
  },
  "keywords": [
    "nodejs",
    "modular"
  ],
  "author": "Raghu Chaudhary",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "bull": "^4.12.6",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "fs-extra": "^11.2.0",
    "handlebars": "^4.7.8",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.4.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.13",
    "pretty-error": "^4.0.0",
    "reflect-metadata": "^0.2.2",
    "routing-controllers": "^0.10.4",
    "strong-error-handler": "^5.0.10",
    "typedi": "^0.10.0",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/mongoose": "^5.11.97",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.12.12",
    "@types/nodemailer": "^6.4.15",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2"
  }
}
